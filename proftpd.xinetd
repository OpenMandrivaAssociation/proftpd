# default: off
# description: proftpd server, xinetd version. \
# Don't run the standalone version if you run \
# this!

service ftp
{
	socket_type		= stream
	wait			= no
	user			= root
	server			= /usr/sbin/in.ftpd
	log_on_success		+= DURATION USERID
	log_on_failure		+= USERID
	nice			= 10
	disable			= yes
}
